{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/AccessControl.AppService/AccessControl.AppService.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/src/AccessControl.AppService/AccessControl.AppService.csproj"
            ],
            "windows": {
                "command": "${workspaceFolder}\\src\\AccessControl.AppService\\publish.bat"
            },
            "osx": {
                "command": "${workspaceFolder}/src/AccessControl.AppService/publish.sh"
            },
            "problemMatcher": "$tsc"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/src/AccessControl.AppService/AccessControl.AppService.csproj"
            ],
            "problemMatcher": "$tsc"
        }
    ]
}